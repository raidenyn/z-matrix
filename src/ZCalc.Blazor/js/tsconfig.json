{
  "compilerOptions": {
    "importHelpers": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "target": "ES6"
  },
  "exclude": [
    "node_modules"
  ]
}
