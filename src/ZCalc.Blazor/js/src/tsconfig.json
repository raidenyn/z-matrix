{
  "compilerOptions": {
    "importHelpers": true,
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "DOM.Iterable",
      "ES2015.Symbol",
      "ES2015.Iterable",
      "ES2015.Collection"
    ],
    "module": "ES2020",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "target": "ES2020"
  },
  "include": [
    "**/*.ts"
  ]
}
